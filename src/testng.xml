<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="GDMobileAutomation">
    <listeners>
        <listener class-name="listeners.ListenersClass"/>
    </listeners>
    <test parallel="methods" thread-count="1" name="Test">
        <parameter name="platformName" value="Android"/>
        <classes>
            <class name="tests.CreateUserAccount"/>
            <class name="tests.UpdateUserAccount"/>
        </classes>
    </test>
</suite>
